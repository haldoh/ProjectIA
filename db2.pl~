% distanze fra armadi
dist(l(b), l(a1), 10).
dist(l(b), l(a2), 10).
dist(l(a1), l(a2), 4).
dist(l(a1), l(a3), 6).
dist(l(a2), l(a4), 6).
dist(l(a3), l(a4), 4).
dist(l(a3), l(a5), 6).
dist(l(a4), l(a6), 6).
dist(l(a5), l(a6), 4).

% posizione articoli
contiene(a1, art(1), 10).
contiene(a1, art(2), 10).
contiene(a2, art(3), 10).
contiene(a2, art(4), 10).
contiene(a3, art(5), 10).
contiene(a3, art(6), 10).
contiene(a4, art(7), 10).
contiene(a4, art(8), 10).
contiene(a5, art(9), 10).
contiene(a5, art(10), 10).
contiene(a6, art(11), 10).
contiene(a6, art(12), 10).

% capacità trasporto robot
capacita(10).
